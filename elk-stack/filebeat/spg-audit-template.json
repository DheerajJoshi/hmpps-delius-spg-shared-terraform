        "senderControlRef": {"type": "long"}
      },
      "dynamic_templates" : [
      { 
        "dates" : {
          "match" : "interchangeTimestamp",
          "mapping" : {
            "type" : "date",
            "format" : "YYYY-MM-dd'T'HH:mm:ss.SSSZ"
          }
        }
      }
      ]
  }
}

